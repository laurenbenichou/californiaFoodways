        <%= image_tag("banner_cream.jpg") %>
      </div>
    </div>
        <div class="col-xs-5 text-center">
          <nav>
            <ul id="navigation">
              <li>Home</li>
              <li>About</li>
              <li> Support</li>
              <li>Social</li>
            </ul>
          </nav>
      </div>
  </div>



  <!-- body -->
  <div ng-view>
    <%= yield %>
  </div>
</body>
</html>
